{
    "name": "artisanry/browsershot",
    "description": "Take a screenshot of a website",
    "keywords": ["laravel", "framework", "Laravel-Browsershot", "Laravel Browsershot"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.6",
        "intervention/image": "^2.4"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Browsershot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Browsershot\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Browsershot\\BrowsershotServiceProvider"
            ],
            "aliases": {
                "Browsershot": "Artisanry\\Browsershot\\Facades\\Browsershot"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
