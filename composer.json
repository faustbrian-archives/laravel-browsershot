{
    "name": "artisanry/browsershot",
    "description": "Take a screenshot of a website",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Browsershot",
        "Laravel Browsershot"
    ],
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8",
        "intervention/image": "^2.4"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Browsershot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Browsershot\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Browsershot\\BrowsershotServiceProvider"
            ],
            "aliases": {
                "Browsershot": "Artisanry\\Browsershot\\Facades\\Browsershot"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
